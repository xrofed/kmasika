{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "public/**",
          "views/**",
          "models/**",
          "routes/**",
          "middleware/**",
          "bot/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/app.js" }
  ]
}
